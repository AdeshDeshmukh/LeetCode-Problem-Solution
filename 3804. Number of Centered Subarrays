using ll   =  long long;
using vll  =  vector<ll>;
#define foo(i, a, b) for (int i = (a); i < (b); ++i)

class Solution {
public:
    int centeredSubarrays(vector<int>& n) {
        ll nn = n.size() , re = 0 ;
        foo(i,0,nn){
            ll s = 0 ;
            unordered_set<ll> f ;
            foo(j,i,nn){
                s += n[j] ;
                f.insert(n[j]) ;
                if(f.count(s)) re++ ;
            }
        }
        return re ;
    }
};
