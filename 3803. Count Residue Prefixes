using ll   =  long long;
using vll  =  vector<ll>;

class Solution {
public:
    int residuePrefixes(string s) {
        ll a = 0 , d = 0 ;
        bool so[26] =  {false} ;

        for(int i = 0 ; i < s.size() ; i++){
            ll idx = s[i] - 'a' ;
            if(!so[idx]){
                so[idx] = true ;
                d++ ;

                
            }
            if(d == (i+1) %3) a++ ;
        } 
        return a ;
    }
};
