// Contest LC
using ll   =  long long;
using vi  =  vector<int>;
using vll  =  vector<ll>;
#define pb push_back
#define sorta(v) sort((v).begin(), (v).end())
#define foo(i, a, b) for (int i = (a); i < (b); ++i )


class Solution {
public:
    int minRemoval(vi& n, int k) {
        sorta(n);
        int re = n.size();
        foo(i,0,n.size()) {
            int j = upper_bound(n.begin(), n.end(), (long)n[i] * k) - n.begin();
            re = min(re, i + (int)n.size() - j);
        }
        return re;
    }
};
