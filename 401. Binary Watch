// Contest LC
using ll   =  long long;
using vi  =  vector<int>;
using vll  =  vector<ll>;
#define pb push_back
#define sorta(v) sort((v).begin(), (v).end())
#define foo(i, a, b) for (int i = (a); i < (b); ++i )


class Solution {
public:
    vector<string> readBinaryWatch(int turnedOn) {
        vector<string> ans;
        foo(h,0,12) {
            foo(m,0,60) {
                if (__builtin_popcount(h) + __builtin_popcount(m) == turnedOn) {
                    ans.pb(to_string(h) + ":" + (m < 10 ? "0" : "") +
                                  to_string(m));
                }
            }
        }
        return ans;
    }
};
